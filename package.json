{
  "name": "engine",
  "version": "0.0.1",
  "description": "",
  "repository": "",
  "author": "Tofunmi <https://github.com/tofunmiakin>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc && npm run copy-views",
    "copy-views": "copyfiles -u 1 views/**/* dist/views",
    "start": "node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/data-source.ts",
    "schema:sync": "yarn typeorm schema:sync -- -d src/data-source.ts",
    "migration:show": "yarn typeorm migration:show -- -d src/data-source.ts",
    "migration:generate": "yarn typeorm migration:generate -- -d src/data-source.ts",
    "migration:create": "yarn typeorm migration:create",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
    "build:compile": "npx babel src --extensions .ts --out-dir build --source-maps",
    "build:types": "tsc",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "prestart:dev": "yarn db:dev:start",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">1.20.0",
    "npm": "pleas-use-yarn"
  },
  "dependencies": {
    "@sentry/node": "^10.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.1.3",
    "@types/nodemailer": "^6.4.9",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/pg": "^8.10.1",
    "@types/pug": "^2.0.6",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "google-auth-library": "^9.4.1",
    "http-status-codes": "^2.2.0",
    "husky": "^8.0.3",
    "joi": "^17.12.3",
    "jsonwebtoken": "^9.0.1",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.4",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.0",
    "pug": "^3.0.2",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/node": "^7.22.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.2",
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.22.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-source-map-support": "^2.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "lint-staged": "^12.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier src -w"
    ]
  }
}
